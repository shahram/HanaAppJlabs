{
    "name": "HanaAppJlabs",
    "version": "1.0.0",
    "description": "A simple CAP project.",
    "repository": "<Add your repository here>",
    "license": "UNLICENSED",
    "private": true,
    "dependencies": {
        "@sap/cds": "^5",
        "@sap/hana-client": "^2.11.20",
        "express": "^4",
        "hdb": "^0.19.1"
    },
    "scripts": {
        "start": "cds run",
        "undeploy": "cf undeploy HanaAppJlabs --delete-services --delete-service-keys"
    },
    "eslintConfig": {
        "extends": "eslint:recommended",
        "env": {
            "es2020": true,
            "node": true,
            "jest": true,
            "mocha": true
        },
        "globals": {
            "SELECT": true,
            "INSERT": true,
            "UPDATE": true,
            "DELETE": true,
            "CREATE": true,
            "DROP": true,
            "CDL": true,
            "CQL": true,
            "CXL": true,
            "cds": true
        },
        "rules": {
            "no-console": "off",
            "require-atomic-updates": "off"
        }
    },
    "cds": {
        "build": {
            "target": "."
        },
        "requires": {
            "db": {
                "kind": "hana"
            }
        },
        "hana": {
            "deploy-format": "hdbtable"
        }
    },
    "devDependencies": {
        "@sap/hdi-deploy": "4.3.0",
        "@sap/ux-specification": "^1.96.5"
    },
    "sapux": [
        "app/frontend-jlabs"
    ]
}
