{
    "name": "HanaAppJlabs",
    "version": "1.0.0",
    "description": "Runtime environment needed for deployment",
    "repository": {},
    "private": "true",
    "dependencies": {
        "@sap/audit-logging": "5.3.0",
        "@sap/cds": "5.7.4",
        "@sap/hana-client": "2.11.20",
        "@sap/xsenv": "3.1.1",
        "@sap/xssec": "3.2.11",
        "express": "4.17.3",
        "passport": "0.5.2"
    },
    "scripts": {
        "start": "cds serve gen/csn.json",
    },
    "engines": {
        "node": "14.X",
    },
    "cds": {
        "build": {
            "target": "."
        },
        "hana": {
            "deploy-format": "hdbtable"
        },
        "requires": {
            "db": {
                "kind": "hana"
            },
            "uaa": {
                "kind": "xsuaa"
            }
        }
    }
}